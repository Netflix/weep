<plist version="1.0"> 
<dict> 
    <key>Label</key> 
    <string>com.user.lo0-loopback</string> 
    <key>ProgramArguments</key> 
    <array> 
        <string>/sbin/ifconfig</string> 
        <string>lo0</string> 
        <string>alias</string> 
        <string>169.254.169.254</string> 
    </array> 
    <key>RunAtLoad</key> <true/> 
    <key>Nice</key> 
